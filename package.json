{
  "name": "historypin-internet-archive",
  "version": "1.0.0",
  "private": true,
  "description": "an application that allows one to upload historypin collections to the internet archive",
  "scripts": {
    "browserify": "browserify ./app/assets/js/app.js -o ./public/js/app.js",
    "build:js": "npm run jshint && npm run browserify && npm run uglifyjs",
    "jshint": "jshint .",
    "start": "npm run build:js && nodemon ./bin/www & npm run watchify",
    "uglifyjs": "uglifyjs ./public/js/app.js -c drop_console=true,warnings=false -o ./public/js/app.min.js",
    "watchify": "watchify ./app/assets/js/app.js -o ./public/js/app.js"
  },
  "author": "dan entous",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.3.3",
    "body-parser": "^1.15.0",
    "browserify": "^13.0.0",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "cookie-session": "^1.2.0",
    "csurf": "^1.8.3",
    "express": "^4.13.4",
    "hjs": "0.0.6",
    "jshint": "^2.9.1",
    "morgan": "^1.7.0",
    "node-accepted-languages": "git+https://github.com/dan-nl/node-accepted-languages.git",
    "node-create-file": "git+https://github.com/dan-nl/node-create-file.git",
    "node-error-logger": "git+https://github.com/dan-nl/node-error-logger.git",
    "node-file-exists": "git+https://github.com/dan-nl/node-file-exists.git",
    "node-front-end-lib": "git+https://github.com/dan-nl/node-front-end-lib.git",
    "node-historypin": "git+https://github.com/Historypin/node-historypin.git",
    "node-lang": "git+https://github.com/dan-nl/node-lang.git",
    "node-sass-middleware": "^0.9.7",
    "request": "^2.69.0",
    "request-debug": "^0.2.0",
    "uglify-js": "^2.6.2",
    "validator": "^5.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.9.1",
    "watchify": "^3.7.0"
  }
}
